import java.util.*;
public class Main
{
	public static void main(String[] args) {
		Scanner s = new Scanner(System.in);
		int r = s.nextInt();
		int num;
		for(int i=1;i<=r;i++){
		    num = i%2;
		    System.out.print(num+" ");
		    for(int j=1;j<i;j++){
		        if(num==1)
		            num = 0;
		        else
		            num = 1;
		        System.out.print(num+" ");
		    }
		    System.out.println();
		}
	}
}
