import java.util.*; 
public class Main
{
	public static void main(String[] args) {
	    Scanner s = new Scanner(System.in);
	    int num = s.nextInt();
	    int rem;
	    String rev="";
	    while(num!=0){
	        rem = num%10;
	        rev += rem; 
	        num /= 10;
	    }
	    int res = Integer.valueOf(rev);
	    System.out.println(rev);
	}
}
