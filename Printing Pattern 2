import java.util.*;
public class Main
{
	public static void main(String[] args) {
		Scanner s = new Scanner(System.in);
		int r = s.nextInt();
		int count = 1;
		for(int i=1;i<=r;i++){
		    for(int j=1;j<=count;j++){
		        System.out.print("*");
		    }
		    count++;
		    System.out.println();
		}
	}
}
